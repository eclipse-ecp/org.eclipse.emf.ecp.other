                     <test
                           property="org.eclipse.emf.ecp.core.repository.providerName"
                           value="org.eclipse.emf.ecp.emfstore.provider">
                     </test>
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.emf.ecp.emfstore.ui.testers.emfserverIsLoggedIn"
