            <visibleWhen
                  checkEnabled="false">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <test
                           args="org.eclipse.emf.ecp.emfstore.ui.showHistoryView"
                           property="org.eclipse.emf.ecp.ui.workbench.menu.enablement"
                           value="true">
                     </test>
                     <test
