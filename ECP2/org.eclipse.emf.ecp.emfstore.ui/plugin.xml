            <visibleWhen
                  checkEnabled="false">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.emf.ecp.emfstore.ui.testers.isServerAdmin"
                           value="true">
                     </test>
                     <instanceof
                           value="org.eclipse.emf.ecp.emfstore.core.internal.EMFStoreProjectWrapper">
                     </instanceof>
                  </iterate>
               </and>
            </visibleWhen>
         </command>
