<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="org.eclipse.emf.cdo.ecp.provider"
         name="CDO Model Repository"
         point="org.eclipse.emf.ecp.ui.uiProviders">
      <uiProvider
            class="org.eclipse.emf.cdo.ecp.internal.ui.CDOUIProvider"
            provider="org.eclipse.emf.cdo.ecp.provider">
      </uiProvider>
   </extension>

  <extension
        point="org.eclipse.core.runtime.adapters">
     <factory
           adaptableType="org.eclipse.emf.ecp.spi.core.InternalProject"
           class="org.eclipse.emf.cdo.ecp.internal.ui.WorkspacePropertyAdapterFactory">
        <adapter type="org.eclipse.ui.views.properties.IPropertySourceProvider"/>
     </factory>
  </extension>

</plugin>
