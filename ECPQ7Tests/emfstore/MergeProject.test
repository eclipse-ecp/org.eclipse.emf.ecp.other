Content-Type: text/ecl
Entry-Name: .content

with [get-view "Model Explorer" | get-tree] {
    select "TestProjectSecondCheckOut/Tournament Pro/Matchup" | get-menu Game | click
    select TestProjectSecondCheckOut | get-menu Commit | click
}
with [get-window Commit] {
    get-editbox -after [get-label "Log message:"] | set-text TestCommit
    get-button OK | click
}
with [get-view "Model Explorer" | get-tree] {
    select "TestProject/Tournament Pro/Matchup/Game" | double-click
    select "TestProject/League America" | double-click
}
get-editor "League America" | get-editbox -after [get-label Name] | set-text "Americas Superleague"
get-view "Model Explorer" | get-tree | select "TestProjectSecondCheckOut/League America" | double-click
get-editor "League America" | get-editbox -after [get-label Name] | set-text "Americas Looserleague"
get-view "Model Explorer" | get-tree | select TestProject | get-menu Commit | click
get-window Confirmation 
    | get-label "Your project is outdated, you need to update before commit. Do you want to update now?" 
    | get-property caption 
    | equals "Your project is outdated, you need to update before commit. Do you want to update now?" | verify-true
get-window Confirmation | get-button OK | click
get-window Update | get-button OK | click
with [get-window Commit] {
    get-editbox -after [get-label "Log message:"] | set-text TEST
    get-button OK | click
}
get-view "Model Explorer" | get-tree | select TestProjectSecondCheckOut | get-menu Commit | click
get-window Confirmation | get-button OK | click
with [get-window "Merge Wizard"] {
    get-label "Merge Conflicts" | get-property caption | equals "Merge Conflicts" | verify-true
    get-group "Conflict Description:" | get-editbox | get-property text 
        | equals "You have changed the name attribute of League Americas Looserleague from America to Americas Looserleague. This attribute was changed to Americas Superleague on the repository." 
        | verify-true
}




with [get-window "Merge Wizard"] {
    get-button "Keep All Their Changes" | click
    get-button Finish | click
}
get-window "No local changes" 
    | get-label "Your local changes were mutually exclusive.There are no changes pending for commit." 
    | get-property caption | equals "Your local changes were mutually exclusive.\n"
    + "There are no changes pending for commit." | verify-true
get-window "No local changes" | get-button OK | click
get-editor "League Americas Superleague" | get-editbox -after [get-label Name] | get-property text 
    | equals "Americas Superleague" | verify-true
with [get-view "Model Explorer" | get-tree] {
    get-item "TestProjectSecondCheckOut/League Americas Superleague" | get-property caption 
        | equals "League Americas Superleague" | verify-true
    get-menu Commit | get-property enablement | equals false | verify-true
}
